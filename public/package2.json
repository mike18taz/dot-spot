{
  "name": "connecting-dots",
  "version": "1.0.0",
  "description": "webapp",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run gcp-build",
    "gcp-build": "lb-tsc",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^4.2.8",
    "@loopback/authentication-jwt": "^0.4.2",
    "@loopback/boot": "^2.3.4",
    "@loopback/context": "^3.9.1",
    "@loopback/core": "^2.9.0",
    "@loopback/openapi-v3": "^3.4.4",
    "@loopback/repository": "^2.8.0",
    "@loopback/rest": "^5.1.2",
    "@loopback/rest-explorer": "^2.2.5",
    "@loopback/service-proxy": "^2.3.3",
    "loopback-connector-mongodb": "^4.2.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.0.0",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.1.8",
    "@types/node": "^10.17.26",
    "@typescript-eslint/parser": "^2.31.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@loopback/eslint-config": "^8.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "typescript": "~3.9.5"
  }
}
